{
  "consumer": {
    "name": "MobileApp"
  },
  "interactions": [
    {
      "description": "A GET request to fetch a booking",
      "request": {
        "method": "GET",
        "path": "/booking/BNK48",
        "query": {
          "firstname": [
            "Alice"
          ]
        }
      },
      "response": {
        "body": {
          "departureTime": "0001-01-01T00:00:00+00:00",
          "firstname": "Alice",
          "pnr": "BNK48"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.departureTime": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.pnr": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pactRust": {
      "ffi": "0.3.3",
      "mockserver": "0.9.2",
      "models": "0.4.1"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "FlyWithMeGetBooking"
  }
}